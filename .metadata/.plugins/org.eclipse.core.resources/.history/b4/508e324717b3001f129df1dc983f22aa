package pack2;

import java.util.Scanner;

public class K26 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int i = sc.nextInt();
		int j = sc.nextInt();
		try {
			int c = i / j;
			System.out.println(c);
		}
		catch(Exception ex) {
			ex.printStackTrace();
		}
	}
}
